package ch02;

import java.util.Scanner;

public class pj11_01 {

	public static void main(String[] args) {
		System.out.println("달을 입력>>");
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		switch (num) {
		case 1: {
			System.out.println("겨울");
			
		}
		default:
			System.out.println("12전까지 입력 또는 숫자만 입력");
		}

	}

}
