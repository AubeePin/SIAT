package ch02;

import java.util.Scanner;

public class pj11_01 {

	public static void main(String[] args) {
		System.out.println("달을 입력>>");
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		switch (num) {
		case 3: 
		case 4:
		case 5:{
			System.out.println("봄");
		}
		
		}

	}

}
